{
  "name": "aybarsm/laravel-pandle",
  "description": "Laravel service provider for Pandle (pandle.com) bookkeeping platform API.",
  "keywords": ["php", "laravel", "pandle", "sdk", "api", "client"],
  "license": "MIT",
  "homepage": "https://github.com/aybarsm/laravel-pandle",
  "authors": [
    {
      "name": "Murat Aybars",
      "email": "hello@aybarsm.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.37.0"
  },
  "require-dev": {
    "laravel/pint": "^1.18.1",
    "pestphp/pest": "^3.5.0",
    "pestphp/pest-plugin-arch": "^3.0.0",
    "phpstan/phpstan": "^1.12.7",
    "symfony/var-dumper": "^7.2.0"
  },
  "autoload": {
    "psr-4": {
      "Aybarsm\\Laravel\\Pandle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": false
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Aybarsm\\Laravel\\Pandle\\PandleServiceProvider"
        ]
    }
  }
}